<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="org.example.fifthlab.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button text="Перекидыватель слов" onAction="#onThrowerButtonClick"/>
    <Button text="Пропадающие виджеты" onAction="#onWidgetsButtonClick"/>
    <Button text="Ресторан" onAction="#onRestaurantButtonClick"/>
    <Button text="Калькулятор" onAction="#onCalculatorButtonClick"/>
    <Button text="Флаг" onAction="#onFlagButtonClick"/>

</VBox>
